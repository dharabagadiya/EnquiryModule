
@{
    ViewBag.Title = "Services";
}
<style>
    .footer_new { position: relative !important; }
</style>
<div class="breadcrumb left">
    <span class=""><a href="@Url.Action("","Home")"> Home </a></span>
    <span class="marg_arrow"><img src="~/images/arrow.png" /></span>
    <span class=""><a href="@Url.Action("", "RenewServices")"> Place Search Request </a></span>
    <span class="marg_arrow"><img src="~/images/arrow.png" /></span>
    <span class=""><a href="@Url.Action("Services", "Wind")"> Wind Services </a></span>
</div>
<div class="form_slider_wrapper">
    <span class="prev_slide"></span>
    <span class="next_slide"></span>
    <div class="container">
        <div id="wind_renew_form_slider" class="form_slider">
            <div id="wind_renew_slides_group" class="slides_group">
                <div id="wind_renew_form_slide_1" class="slides">
                    <h1 class="slide_title">Location of the Wind Farm</h1>
                    <div class="input_frame">
                        <div class="input_element_holder multiple_fields">
                            <div class="MB15">
                                <div class="icon_input textarea">
                                    <span class="icon"><span class="sprite_img icon_size add_img"></span></span>
                                    <textarea id="wind_renew_address" class="no_label grey_text" placeholder="Address"></textarea>
                                </div>
                                <span id="add123" class="left_arrow"></span>
                            </div>
                            <div class="MB15">
                                <div class="icon_input">
                                    <span class="icon"><span class="sprite_img icon_size add_img"></span></span>
                                    <input id="txtLocation" class="no_label grey_text" type="text" placeholder="Location" />
                                </div>
                                <span id="loc123" class="left_arrow"></span>
                            </div>
                            <div class="MB15">
                                <div class="icon_input">
                                    <span class="icon"><span class="sprite_img icon_size pin_img"></span></span>
                                    <input id="wind_renew_pincode" class="no_label grey_text" type="text" placeholder="Pincode" onkeypress="return isNumberKey(event)" data-minlength="6" maxlength="6" />
                                </div>
                                <span id="pin123" class="left_arrow" style="margin-left:-130px;"></span>
                            </div>
                        </div>
                    </div>
                    <div class="clr"></div>
                    <div class="btn_area">
                        <button class="btn_normal_2 continue_btn">Continue</button>
                    </div>
                </div>

                <div id="wind_renew_form_slide_2" class="slides">
                    <h1 class="slide_title" style="padding:15px 0 15px 0 !important;">Service you are looking for</h1>
                    <div class="input_frame">
                        <div id="wind_ServiceLookingType" class="option_holder radio_options small_options four_col ServiceLookingType">
                            <div id="wind_renew_turbine_component" class="option_box">
                                <div class="icon margin_icon component_bg"><div class="solar_renew_img img_size component_img"></div></div>

                                <span class="selectable"></span>
                                <h3 class="label">Turbine Component</h3>
                            </div>
                            <div id="wind_renew_turbine_services" class="option_box">
                                <div class="icon margin_icon finance_bg"><div class="solar_renew_img img_size services_img"></div></div>

                                <span class="selectable"></span>
                                <h3 class="label">Turbine Services</h3>
                            </div>

                            <div id="wind_renew_training" class="option_box">
                                <div class="icon margin_icon training_bg"><div class="solar_renew_img img_size training_img"></div></div>

                                <span class="selectable"></span>
                                <h3 class="label">Training</h3>
                            </div>

                            <div id="wind_renew_logistics" class="option_box">
                                <div class="icon margin_icon logistic_bg"><div class="solar_renew_img img_size logistic_img"></div></div>

                                <span class="selectable"></span>
                                <h3 class="label">Logistic Services</h3>
                            </div>

                            <div class="clr"></div>

                            <div id="wind_renew_business" class="option_box second_option_line">
                                <div class="icon margin_icon business_bg"><div class="solar_renew_img img_size business_img"></div></div>

                                <span class="selectable"></span>
                                <h3 class="label">Business &amp; Development</h3>
                            </div>
                            <div id="wind_renew_recruitment" class="option_box">
                                <div class="icon margin_icon staff_bg"><div class="solar_renew_img img_size staff_img"></div></div>

                                <span class="selectable"></span>
                                <h3 class="label">Staff &amp; Recruitment</h3>
                            </div>

                            <div id="wind_renew_other_services" class="option_box">
                                <div class="icon margin_icon others_bg"><div class="solar_renew_img img_size others_img"></div></div>

                                <h3 class="label">Others</h3>
                                <input id="wind_renew_others" type="text" class="input top_10" />
                            </div>
                            <div class="clr"></div>
                        </div>
                    </div>
                    <div class="clr"></div>
                    <span id="serviceLooking" class="left_arrow" style="margin-left:-7px;"></span>
                    <div class="btn_area" style="padding-top:20px !important;">
                        <button class="btn_normal_2 continue_btn">Continue</button>
                    </div>
                </div>

                <div id="wind_renew_form_slide_3" class="slides">
                    <h1 class="slide_title" style="padding:15px 0 15px 0 !important;">Select Wind Turbine Manufacturer you are using</h1>
                    <div class="input_frame">
                        <div id="wind_TurbineManufacture" class="option_holder radio_options small_options four_col TurbineManufacture">
                            <div id="wind_gamesa" class="option_box">
                                <img class="icon" src="~/images/govt_icon.png" />
                                <span class="selectable"></span>
                                <h3 class="label">Gamesa</h3>
                            </div>
                            <div id="wind_clipper" class="option_box">
                                <img class="icon" src="~/images/industries_icon.png" />
                                <span class="selectable"></span>
                                <h3 class="label">Clipper</h3>
                            </div>
                            <div id="wind_suzlon" class="option_box">
                                <img class="icon" src="~/images/corporate_icon.png" />
                                <span class="selectable"></span>
                                <h3 class="label">Suzlon</h3>
                            </div>
                            <div id="wind_vestas" class="option_box">
                                <img class="icon" src="~/images/individual_icon.png" />
                                <span class="selectable"></span>
                                <h3 class="label">Vestas</h3>
                            </div>
                            <div class="clr"></div>
                            <div id="wind_ge_energy" class="option_box second_option_line">
                                <img class="icon" src="~/images/trust_icon.png" />
                                <span class="selectable"></span>
                                <h3 class="label">GE Energy</h3>
                            </div>
                            <div id="wind_acciona" class="option_box">
                                <img class="icon" src="~/images/corporate_icon.png" />
                                <span class="selectable"></span>
                                <h3 class="label">Acciona</h3>
                            </div>
                            <div id="wind_staff_others" class="option_box">
                                <img class="icon" src="~/images/individual_icon.png" />
                                <span class="selectable"></span>
                                <h3 class="label">Others</h3>
                                <input id="wind_staff_other" type="text" class="input top_10" />
                            </div>
                            <div class="clr"></div>
                        </div>
                    </div>
                    <div class="clr"></div>
                    <span id="turbineManufacturer" class="left_arrow" style="margin-left:-7px;"></span>
                    <div class="btn_area" style="padding-top:20px !important;">
                        <button class="btn_normal_2 continue_btn">Continue</button>
                    </div>
                </div>

                <div id="wind_renew_form_slide_4" class="slides">
                    <h1 class="slide_title">Service request details</h1>
                    <div class="input_frame">
                        <div class="input_element_holder multiple_fields">
                            <div class="MB15">
                                <div class="icon_input">
                                    <span class="icon"><img src="~/images/location_icon_white.png" /></span>
                                    <input id="wind_renew_service" class="no_label grey_text" type="text" placeholder="Type Service you are Looking for" />
                                </div>
                                <span id="serviceType" class="left_arrow" style="margin-left:-75px;"></span>
                            </div>
                            <div class="MB15">
                                <div class="icon_input textarea">
                                    <span class="icon"><span class="sprite_img icon_size item_img"></span></span>
                                    <textarea id="wind_renew_msg" class="no_label grey_text" placeholder="Please provide information on your requested item(s)"></textarea>
                                </div>
                                <span id="serviceMsg" class="left_arrow" style="margin-left:-130px;"></span>
                            </div>
                        </div>
                    </div>
                    <div class="clr"></div>
                    <div class="btn_area">
                        <button class="btn_normal_2 continue_btn">Continue</button>
                    </div>
                </div>

                <div id="wind_renew_form_slide_5" class="slides">
                    <h1 class="slide_title">Contact details</h1>
                    <div class="input_frame">
                        <div class="input_element_holder multiple_fields">
                            <div class="MB15">
                                <div class="icon_input">
                                    <span class="icon"><span class="sprite_img icon_size com_img"></span></span>
                                    <input id="wind_renew_company_name" class="no_label grey_text" type="text" placeholder="Individual/Company Name" />
                                </div>
                                <span id="companyName" class="left_arrow"></span>
                            </div>
                            <div class="MB15">
                                <div class="icon_input">
                                    <span class="icon"><span class="sprite_img icon_size contact_img"></span></span>
                                    <input id="wind_renew_contact_person" class="no_label grey_text" type="text" placeholder="Contact Person Name" />
                                </div>
                                <span id="contactName" class="left_arrow" style=" margin-left: -111px;"></span>
                            </div>
                            <div class="MB15">
                                <div class="icon_input">
                                    <span class="icon"><span class="sprite_img icon_size email_img"></span></span>
                                    <input id="wind_renew_email" class="no_label grey_text" type="text" placeholder="Email Id" />
                                </div>
                                <span id="email" class="left_arrow" style="margin-left:-195px;"></span>
                            </div>
                            <div class="MB15">
                                <div class="icon_input">
                                    <span class="icon"><span class="sprite_img icon_size mob_img"></span></span>
                                    <input id="wind_renew_mobile" class="no_label grey_text" type="text" placeholder="Mobile Number" onkeypress="return isNumberKey(event)" maxlength="10" />
                                </div>
                                <span id="mobile" class="left_arrow"></span>
                            </div>
                        </div>
                    </div>
                    <div class="clr"></div>
                    <div class="btn_area">
                        <button class="btn_normal_2 continue_btn" id="btnWindServices">Finish</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

